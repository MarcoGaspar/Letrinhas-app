<div class="content grad-blue">
    <div id="msgContainer" style="position:absolute; width:100%; z-index:9999; display:none;" class="alert alert-success" role="alert">
        <p id="syncDBInfo" class="" style="Color:white" class="alert alert-success" role="alert"> </p>
    </div>

    <div class="login-bg " style="color:white; font-size:1.2em;"></div>

    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="pr-wrap">
                    <div class="pass-reset">
                        <label>
                            Enter the email you signed up with</label>
                        <input type="email" placeholder="Email" />
                        <input type="submit" value="Submit" class="pass-reset-submit btn btn-success btn-sm" />
                    </div>
                </div>
                <div class="wrap" style=" margin-top:10%">

                    <div style="width:100%;text-align: center;">
                        <img style="width: 80%; max-width: 350px; margin: 25px 0;" src="images/under-construction.png">
                    </div>
                    <div style="width:100%;text-align: center; color:white; ">
                      <label>Letrinhas</label>
                    </div>

                    <div style="width:50%; text-align:center; margin:0 auto" id="notificationStatus">
                    </div>

                    <form class="login">
                        <input id="username" type="text">
                        <input id="password" type="password">
                        <div class="remember-forgot">
                            <div class="row">
                                <div class="col-md-6">
                                    <div id="checkbox" class="checkbox">
                                        <label>
                                            <input type="checkbox" />
                                            <span data-i18n="login.rememberme"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div style="width:100%;text-align: center;">
                            <input id="sendBtn" type="submit" class="btn btn-success btn-sm" style="width:50%">
                        </div>
                        <!--
                        <input id="registerBtn" type="submit" value="Registar" class="btn btn-success btn-sm" style="float:right; width:35%">
                        -->


                        <div style="width:100%;text-align: center; margin-top:50px">
                            <div class="row">
                                <span id="debugBtn" class="btn btn-success btn-sm" style="width:15%; text-align:center"><i class="fa fa-cog" aria-hidden="true"></i></span>
                            </div>

                        </div>
                        <div class="" style=" width:100%; height:80%; margin:0 auto; margin-top:50px">

                            <div style="width:99%; height:80% ; text-align: center;float:left;">
                                <img style="width: 80%; max-width: 400px" src="images/partners/ipt.png">
                            </div>

                            <!--
                          <div style="width:30%; height:100% ; text-align: center;float:left">
                              <img style="width: 100%; max-width: 50px;" src="images/partners/estt.png">
                          </div>
                          <div style="width:30%; height:100% ; text-align: center;float:left">
                              <img style="width: 100%; max-width: 50px;" src="images/partners/esag.png">
                          </div>
                              -->
                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>

</div>


<div id="firstScreen" style="display: none; background: transparent; position: relative; margin:0 auto; width:100%; height:100%; z-index:5999">

    <div class="Aligner grad-blue menu-help" >
        <div class="row">
            <!-- END timeline item
            -->
            <span id="showInfoBtn" class="btn btn-success btn-sm" style="width:80%; text-align:center"></i>Ver base de dados</span>
            <span id="delSessionBtn" class="btn btn-success btn-sm" style="width:80%; text-align:center; margin-top:5px"></i>Estou com dificuldades</span>
            <span id="clearLocalDbBtn" class="btn btn-success btn-sm" style="width:80%;  text-align:center; margin-top:5px"></i>Apagar Informação Local</span>

            <span id="closeBtn" class="btn btn-warning btn-sm" style="width:80%; text-align:center; margin-top:15px"></i>Voltar</span>
        </div>
</div>
</div>
