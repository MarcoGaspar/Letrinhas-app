<div id="wrapper">

    <!-- Sidebar -->
    <div id="sidebar-wrapper">

        <nav class="navbar navbar-default" style="margin-bottom:0;">
            <div class="container-fluid">
                <div class="navbar-header">
                    <p class="navbar-text" style="color:white;">
                        <i class="fa fa-caret-square-o-down" aria-hidden="true"></i>
                        <span data-i18n="sidebar.header"></span>
                    </p>
                </div>
            </div>
        </nav>

        {{#account}}
        <ul class="sidebar-nav">

            <li class="sidebar-brand">

                <div class="pull-left image">
                    <img src="{{b64}}" class="img-circle" alt="User Image">
                </div>

                <div class="pull-left info">
                    <p data-i18n="sidebar.welcome"></p>
                    <a>{{name}}</a>
                </div>

            </li>

            <li class="header" data-i18n="sidebar.mainmenu"></li>

            <li class="active">
                <a href="#home">
                    <i class="fa fa-tachometer" aria-hidden="true"></i>
                    <span data-i18n="sidebar.dashboard"></span>
                </a>
            </li>
            <li>
                <a href="#statistics">
                    <i class="fa fa-area-chart" aria-hidden="true"></i>
                    <span data-i18n="sidebar.statistics"></span>
                </a>
            </li>

            <li class="header" data-i18n="sidebar.appmenu"></li>

            <li class="sync">
                <a href="#">
                    <i class="fa fa-refresh" aria-hidden="true"></i>
                    <span data-i18n="sidebar.sync"></span>
                </a>
            </li>

            <li class="header" data-i18n="sidebar.accountmenu"></li>

            <li>
                <a href="#profile">
                    <i class="fa fa-user" aria-hidden="true"></i>
                    <span data-i18n="sidebar.profile"></span>
                </a>
            </li>

            <li>
                <a href="#logout">
                    <i class="fa fa-sign-out" aria-hidden="true"></i>
                    <span data-i18n="sidebar.logout"></span>
                </a>
            </li>

            <!--<li>


            <li class="reloadd">
                <a href="#">
                    <i class="fa fa-user" aria-hidden="true"></i>
                    <span> reload</span>
                </a>
            </li>
          -->
                <!--<a href="#debug">-->
                    <!--<i class="fa fa-sign-out" aria-hidden="true"></i>-->
                    <!--Debug-->
                <!--</a>-->
            <!--</li>-->

        </ul>
        {{/account}}
    </div>
    <!-- /#sidebar-wrapper -->

    <!-- Page Content -->
    <div id="page-content-wrapper">

        <!--CONTENT NAVBAR-->
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">
                        <i class="fa fa-bars" aria-hidden="true"></i>
                    </a>
                    <p class="navbar-text" style="color:white;">Letrinhas</p>
                </div>
            </div>
        </nav>
        <!-- /#CONTENT NAVBAR-->


        <div class="container-fluid chronologyInfo">

            <div class="bs-callout bs-callout-success">
                <h4 class="callout-progress-csp">
                    <i class="fa fa-sort-amount-desc" style="padding-right:10px;" aria-hidden="true"></i>
                    <span data-i18n="lang.chronology"></span>
                    <i class="fa fa-refresh refreshChronology" style="padding-right:10px; float:right; cursor:pointer" aria-hidden="true"></i>
                </h4>
            </div>

            <ul class="timeline">
                <li style="padding-bottom:20px;">
                    <i class="fa fa-clock-o bg-gray"></i>
                </li>
            </ul>

        </div>

    </div>
    <!-- /#page-content-wrapper -->

</div>
<!-- /#wrapper -->
