<header class="bar bar-nav" style="padding:0; background:#488AC4; color:#fff; border: none;">
    <a class="icon pull-left" style="color:white; padding-left:10px;"><i class="fa fa-refresh" aria-hidden="true"></i></a>
    <h1 class="title" style="color:#fff;">
        <i class="fa fa-graduation-cap" aria-hidden="true"></i>
        Letrinhas v3
    </h1>
</header>


<footer>
    <nav class="bar bar-tab" style="background:#488AC4; color:#fff; border: none;">
        <a class="tab-item active" href="#{{url.[0]}}/{{url.[1]}}">
            <span class="icon"><i class="fa fa-line-chart" aria-hidden="true"></i></span>
            <span class="tab-label">Estatisticas</span>
        </a>
        <a class="tab-item" href="#">
            <span class="icon icon-person"></span>
            <span class="tab-label">Prefil</span>
        </a>
        <a id="btnHelp" class="tab-item" href="#">
            <span class="icon icon-info"></span>
            <span class="tab-label">Ajuda</span>
        </a>
        <a class="tab-item" href="#logout">
            <span class="icon icon-download"></span>
            <span class="tab-label">Sair</span>
        </a>
    </nav>
</footer>


<div class="content">

    <div class="outer">
        <div class="middle">
            <div class="inner">

                <table class="highchart" data-graph-container-before="1" data-graph-type="line" style="display: none; height:0 !important; overflow:hidden;">
                    <thead>
                    <tr>
                        <th>Data</th>
                        <th>Nota</th>
                        {{#if fluidity}}<th>Fluidez</th>{{/if}}
                        {{#if accuracy}}<th>Exactid√£o</th>{{/if}}
                    </tr>
                    </thead>
                    <tbody>

                    <tr>
                        <td>01/01/2016 - 00:00:00</td>
                        <td>0%</td>
                        {{#if fluidity}}<td>0%</td>{{/if}}
                        {{#if accuracy}}<td>0%</td>{{/if}}
                    </tr>

                    {{#each results}}
                    <tr>
                        <td>{{resolutionDate}}</td>
                        <td>{{note}}%</td>
                        {{#if answer.fluidity}}<td>{{answer.fluidity}}%</td>{{/if}}
                        {{#if answer.accuracy}}<td>{{answer.accuracy}}%</td>{{/if}}
                    </tr>
                    {{/each}}

                    </tbody>
                </table>

            </div>
        </div>
    </div>




</div>