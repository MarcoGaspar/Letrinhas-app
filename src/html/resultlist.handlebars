<header class="bar bar-nav" style="padding:0; background:#488AC4; color:#fff; border: none;">
    <a class="icon pull-left" style="color:white; padding-left:10px;"><i class="fa fa-refresh" aria-hidden="true"></i></a>
    <h1 class="title" style="color:#fff;">
        <i class="fa fa-graduation-cap" aria-hidden="true"></i>
        Letrinhas v3
    </h1>

    <ol class="breadcrumb" style="background:#f2f2f2">
        <li><a href="#home">
            <i class="fa fa-book" aria-hidden="true"></i>
            Disciplinas
        </a></li>
        <li><a href="#dashboard/{{url.[0]}}">
            <i class="fa fa-bars" aria-hidden="true"></i>
            {{url.[0]}}
        </a></li>
        <li><a href="#{{url.[0]}}/{{url.[1]}}">
            <i class="fa fa-graduation-cap" aria-hidden="true"></i>
            Turmas
        </a></li>
        <li><a href="#{{url.[0]}}/{{url.[1]}}/{{url.[2]}}">
            <i class="fa fa-users" aria-hidden="true"></i>
            Alunos
        </a></li>
        <li class="active"></li>
    </ol>

</header>


<footer>
    <nav class="bar bar-tab" style="background:#488AC4; color:#fff; border: none;">
        <a class="tab-item active" href="#{{url.[0]}}/{{url.[1]}}">
            <span class="icon"><i class="fa fa-line-chart" aria-hidden="true"></i></span>
            <span class="tab-label">Resultados</span>
        </a>
        <a class="tab-item" href="#">
            <span class="icon icon-person"></span>
            <span class="tab-label">Prefil</span>
        </a>
        <a id="btnHelp" class="tab-item" href="#">
            <span class="icon icon-info"></span>
            <span class="tab-label">Ajuda</span>
        </a>
        <a class="tab-item" href="#logout">
            <span class="icon icon-download"></span>
            <span class="tab-label">Sair</span>
        </a>
    </nav>
</footer>


<div class="content">

    <ul class="table-view">
        {{#results}}
        <li class="table-view-cell media">
            <a href="#{{../url.[0]}}/{{../url.[1]}}/{{../url.[2]}}/{{../url.[3]}}/{{_id}}" class="navigate-right">
                <img class="media-object pull-left" src="images/{{../url.[0]}}.png" style="width:75px;">
                <img class="media-object pull-left" src="images/{{type}}.png" style="width:60px; margin-top:10px;">
                <div class="media-body" style="font-size:20px; padding:12px 0 0 5px; line-height:26px">
                    <p style="font-size:20px; color:black;">{{title}}</p>
                    <p style="font-size:17px;">{{description}}</p>
                </div>
            </a>
        </li>
        {{/results}}
    </ul>

</div>